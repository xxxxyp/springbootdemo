<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.springbootdemo.TestDao.TestDao" >
    <select id="findByPrimarkey" resultType="com.example.springbootdemo.TestPojo.TestPojo" parameterType="java.lang.Integer" >
        select * from test_ebay where id = #{id}
    </select>
    <select id="findAll" resultType="com.example.springbootdemo.TestPojo.TestPojo">
        select id,name,age from test_ebay
    </select>
    <select id="allRows" resultType="java.lang.Integer">
        select count(*) from test_ebay
    </select>
